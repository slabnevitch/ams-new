extends _misc/main
block title
	| Каталог курсов

	- var authorized = true;
	- var headerMenu = true;
block content
	.wrapper-simple
		ul.breadcrumbs
			li.breadcrumbs__item
				a(href="").breadcrumbs__link Главная
			li.breadcrumbs__item
				span.breadcrumbs__target Программы обучения

	.page.page-programms
		.wrapper-simple
			.wrapper-flex
				.programms-filters
					.programms-filters__title Фильтры:
					- var checks1 = [{title: 'Revit', val: '15'}, {title: 'Autocad', val: '12'},{title: 'Archicad', val: '27'}, {title: '3Ds Max', val: '7'}, {title: 'Corona', val: '12'}, {title: 'Lumion', val: '3'}];
					- var checks2 = ['Всем', 'Проектировщику', 'Архитектору', 'Дизайнеру', 'Визуализатору', 'Конструктору'];

					- var checks3 = [{title: 'Платные', val: '3'}, {title: 'Бесплатные', val: '3'}];
					form.form
						.programms-filters__block
							.programms-filters__heading Программа
							each item, i in checks1
								label.form__label.form__label--check
									input(type="checkbox" class="form__checkbox" name="name" + (i+1))
									span.form__false-checkbox
										+icon('check')
									span.form__checkbox-label= item.title
										span.programms-filters__value= item.val

							.programms-filters__block
								.programms-filters__heading Кому
								each item, i in checks2
									label.form__label.form__label--check
										input(type="checkbox" class="form__checkbox" name="name" + (i+1))
										span.form__false-checkbox
											+icon('check')
										span.form__checkbox-label= item

							.programms-filters__block
								.programms-filters__heading Доступность
								each item, i in checks3
									label.form__label.form__label--check
										input(type="checkbox" class="form__checkbox" name="name" + (i+1))
										span.form__false-checkbox
											+icon('check')
										span.form__checkbox-label= item.title
											span.programms-filters__value= item.val

			
				.programms-content
					h2 Курсы по Autocad
					.programms-content__block
						.programms-cards-slider.dots-accented
							a(href="/").programm-card.programm-card--autocad
								.programm-card__content
									.programm-card__tags
										.tag.tag-mini.tag-autocad-bg Autocad
										.tag.tag-mini.tag-full-bg Полный курс

									.programm-card__body
										.programm-card__title Использование AutoCAD на 100%  Версия 2.0
										.programm-card__text Идейные соображения высшего порядка, а также перспективное планирование предполагает независимые способы реализации инновационных методов управления процессами. В своём стремлении повысить качество жизни...

									.programm-card__bottom
										.programm-card__exp Без подготовки
										.programm-card__params
											div 13 уроков
											div 20 часов
									.program-icon
										img(src=img + "program-icons/autocad.svg" alt="alt")

								.programm-card__img._fit
									img._fit-img(src=img + "/programms-page/autocad/1.jpg" alt="alt")

							a(href="/").programm-card.programm-card--autocad
								.programm-card__content
									.programm-card__tags
										.tag.tag-mini.tag-autocad-bg Autocad
										.tag.tag-mini.tag-full-bg Полный курс

									.programm-card__body
										.programm-card__title 3D моделирование в AutoCAD
										.programm-card__text Идейные соображения высшего порядка, а также перспективное планирование предполагает независимые способы реализации инновационных методов управления процессами. В своём стремлении повысить качество жизни...

									.programm-card__bottom
										.programm-card__exp Без подготовки
										.programm-card__params
											div 13 уроков
											div 20 часов
									.program-icon
										img(src=img + "program-icons/autocad.svg" alt="alt")

								.programm-card__img._fit
									img._fit-img(src=img + "/programms-page/autocad/2.jpg" alt="alt")

							a(href="/").programm-card.programm-card--autocad
								.programm-card__content
									.programm-card__tags
										.tag.tag-mini.tag-autocad-bg Autocad
										.tag.tag-mini.tag-full-bg Полный курс

									.programm-card__body
										.programm-card__title 3D моделирование в AutoCAD
										.programm-card__text Идейные соображения высшего порядка, а также перспективное планирование предполагает независимые способы реализации инновационных методов управления процессами. В своём стремлении повысить качество жизни...

									.programm-card__bottom
										.programm-card__exp Без подготовки
										.programm-card__params
											div 13 уроков
											div 20 часов

									.program-icon
										img(src=img + "program-icons/autocad.svg" alt="alt")

								.programm-card__img._fit
									img._fit-img(src=img + "/programms-page/autocad/3.jpg" alt="alt")

						.programms-choice-slider.dots-accented
							.your-choice
								.your-choice__wrap
									.your-choice__header
										.tag.tag-mini.tag-autocad-bg Autocad
										.tag.tag-mini.tag-base-bg Бесплатно
									.your-choice__body
										.your-choice__title Базовый курс по 3ds Max + Corona Render
										.your-choice__img._fit
											img._fit-img(src=img + 'you-selection.jpg' alt="alt")
									.your-choice__bottom
										.your-choice__label 13 уроков
										.your-choice__label Без подготовки

							- for (var i=0; i < 2; i++)
								.your-choice
									.your-choice__wrap
										.your-choice__header
											.tag.tag-mini.tag-autocad-bg Autocad
											.tag.tag-mini.tag-base-bg Бесплатно
										.your-choice__body
											.your-choice__title Базовый курс по 3ds Max + Corona Render
											.your-choice__img._fit
												img._fit-img(src=img + 'programms-page/choice2.jpg' alt="alt")
										.your-choice__bottom
											.your-choice__label 13 уроков
											.your-choice__label Без подготовки

						button.btn.btn-bordered.btn-sm Показать ещё

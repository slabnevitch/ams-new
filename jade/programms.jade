extends _misc/main
block title
	| Каталог курсов

	- var authorized = true;
	- var headerMenu = true;
block content
	.wrapper-simple.breadcrumbs-iconed
		ul.breadcrumbs
			li.breadcrumbs__item
				a(href="").breadcrumbs__link Главная
			li.breadcrumbs__item
				span.breadcrumbs__target Программы oбучения
		.filter-open-icon#filter-open
			+icon('filter')

	.page.page-programms


		.wrapper-simple
			.wrapper-flex
				.programms-filters
					.programms-filters__close#close-filter
						+icon('close')

					.programms-filters__title Фильтры:
					- var checks1 = [{title: 'Revit', val: '15'}, {title: 'Autocad', val: '12'},{title: 'Archicad', val: '27'}, {title: '3Ds Max', val: '7'}, {title: 'Corona', val: '12'}, {title: 'Lumion', val: '3'}];
					- var checks2 = ['Всем', 'Проектировщику', 'Архитектору', 'Дизайнеру', 'Визуализатору', 'Конструктору'];

					- var checks3 = [{title: 'Платные', val: '3'}, {title: 'Бесплатные', val: '3'}];
					
					form.form.programms-filters__form
						.programms-filters__block
							.programms-filters__heading Программа
							each item, i in checks1
								label.form__label.form__label--check
									input(type="checkbox" class="form__checkbox" name="name" + (i+1))
									span.form__false-checkbox
										+icon('check')
									span.form__checkbox-label= item.title
										span.programms-filters__value= item.val

							.programms-filters__block
								.programms-filters__heading Кому
								each item, i in checks2
									label.form__label.form__label--check
										input(type="checkbox" class="form__checkbox" name="name" + (i+1))
										span.form__false-checkbox
											+icon('check')
										span.form__checkbox-label= item

							.programms-filters__block
								.programms-filters__heading Доступность
								each item, i in checks3
									label.form__label.form__label--check
										input(type="checkbox" class="form__checkbox" name="name" + (i+1))
										span.form__false-checkbox
											+icon('check')
										span.form__checkbox-label= item.title
											span.programms-filters__value= item.val

			
				.programms-content
					.tag-filters
						- var filters = ['Autocad', '3Ds Max', 'Дизайнеру', 'Сбросить фильтр'];
						each filter in filters
							.tag-fllter
								.tag-fllter__val= filter
								+icon('close')

					.programms-content__block
						h2 Курсы по Autocad
						.programms-cards-slider.dots-accented
							a(href="/").programm-card.bg-autocad
								.programm-card__content
									.programm-card__tags
										.tag.tag-mini.tag-autocad-bg Autocad
										.tag.tag-mini.tag-full-bg Полный курс

									.programm-card__body
										h3 Использование AutoCAD на 100%  Версия 2.0
										.programm-card__text Идейные соображения высшего порядка, а также перспективное планирование предполагает независимые способы реализации инновационных методов управления процессами. В своём стремлении повысить качество жизни...

									.programm-card__bottom
										.programm-card__exp Без подготовки
